{
  "name": "inferno-ts-template",
  "version": "0.0.1",
  "description": "Infernojs + Typescript playground",
  "author": {
    "name": "Christian Linne",
    "email": "ShadowVampire@web.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DorianGrey/inferno-ts-template.git"
  },
  "keywords": [
    "webpack",
    "infernojs",
    "typescript"
  ],
  "devDependencies": {
    "@types/history": "^4.5.0",
    "@types/lodash-es": "^4.14.5",
    "@types/node": "^7.0.12",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-inferno": "^3.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-inferno-app": "^3.0.0",
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "inferno-devtools": "^1.5.4",
    "js-yaml": "^3.8.2",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "tslib": "^1.6.0",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "history": "^4.6.1",
    "inferno": "^1.5.4",
    "inferno-component": "^1.5.4",
    "inferno-mobx": "^1.5.4",
    "inferno-router": "^1.5.4",
    "lodash-es": "^4.17.4",
    "mobx": "^3.1.7",
    "normalize.css": "^6.0.0"
  },
  "scripts": {
    "prestart": "rimraf .tmp",
    "start": "webpack-dev-server --config webpack/main.config.js",
    "start:dist": "npm run dist && node example-dist-server/server.js .dist",
    "start:dist:cc": "npm run dist:cc && node example-dist-server/server.js .dist",
    "predist": "rimraf .dist",
    "dist": "cross-env NODE_ENV=production webpack --config webpack/main.config.js",
    "dist:cc": "npm run dist -- --env.useClosureCompiler",
    "dist:analyze": "npm run dist -- --env.analyze=true",
    "dist:analyze:cc": "npm run dist -- --env.analyze=true --env.useClosureCompiler",
    "translations:dist": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:dev": "npm run translations:dist -- -w"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10",
    "yarn": ">=0.20.0"
  }
}
